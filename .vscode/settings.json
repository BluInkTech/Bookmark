{
    "prettier.enable": false,
    "editor.defaultFormatter": "biomejs.biome",
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[json]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "nodejs-testing.extensions": [
        {
            "extensions": [
                "mjs",
                "cjs",
                "js"
            ],
            "parameters": []
        },
        {
            "extensions": [
                "mts",
                "cts",
                "ts"
            ],
            "parameters": [
                "--import",
                "tsx"
            ]
        }
    ],
    "cSpell.words": [
        "miuy",
        "nodenext",
        "Rajvanshi",
        "Seemant"
    ]
}